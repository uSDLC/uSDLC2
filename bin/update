#!/bin/bash
# Copyright (C) 2012,13 Paul Marrington (paul@marrington.net), see uSDLC2/GPL for license
function update {
  if [ -d ext/node_modules/$1 ]; then
    npm --silent update $1
  else
    npm --silent install $1
  fi
}
update coffee-script
update node-inspector
update stylus
update send
update ensureDir